#include <iostream>
using namespace std;
int main()
{
	int a, b, c;
	cout << "Masukkan bilangan ke 1 : ";
	cin >> a;
	cout << "Masukkan bilangan ke 2 : ";
	cin >> b;
	cout << "Masukkan bolangan ke 3 : ";
	cin >> c;
	if (a < b and a < c) {
		if (b < c) {
			cout << a << b << c;
			return 0;
		} 
		cout << a << c << b;
		return 0;
	}
	else if (b < a and b < c) {
		if (a < c) {
			cout << b << a << c;
			return 0;
		}
		cout << b << c << a;
		return 0;
	}
	else if (c < a and c < b) {
		if (a < b) {
			cout << c << a << b;
			return 0;
		}
		cout << c << b << a;
		return 0;
	}
}
